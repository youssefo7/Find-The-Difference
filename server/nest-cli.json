{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "app",
    "entryFile": "server/app/index",
    "compilerOptions": {
        "plugins": [
            {
                "name": "@nestjs/swagger",
                "options": { "dtoFileNameSuffix": [".dto.ts", ".entity.ts", ".schema.ts"] }
            }
        ]
    }
}
